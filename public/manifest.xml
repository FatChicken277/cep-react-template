<ExtensionManifest Version="7.0" ExtensionBundleId="com.tuempresa.panel" ExtensionBundleVersion="1.0.0">
  <ExtensionList>
    <Extension Id="com.tuempresa.panel"/>
  </ExtensionList>

  <ExecutionEnvironment>
    <HostList>
      <Host Name="PPRO" Version="[0.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="6.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>

  <DispatchInfoList>
    <Extension Id="com.tuempresa.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Mi Panel React</Menu>
          <Geometry>
            <Size>
              <Height>300</Height>
              <Width>400</Width>
            </Size>
          </Geometry>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
